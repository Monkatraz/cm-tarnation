{
  "name": "cm-tarnation",
  "license": "MPL 2.0",
  "description": "An alternative parser for CodeMirror 6",
  "version": "0.1.1",
  "keywords": [
    "wikijump",
    "lezer",
    "codemirror",
    "parsing",
    "parser",
    "textmate",
    "grammar",
    "regex",
    "regexp"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "eslint \"./src/**/*.ts\" --fix && prettier --write --ignore-unknown \"src/**\""
  },
  "type": "module",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*",
    "src/**/*"
  ],
  "browserslist": [
    "supports es6-module and > 0.2% and last 2 versions",
    "Firefox ESR",
    "not dead",
    "not ie > 0"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Monkatraz/cm-tarnation.git"
  },
  "bugs": "https://github.com/Monkatraz/cm-tarnation/issues",
  "dependencies": {
    "@codemirror/highlight": "^0.19.7",
    "@codemirror/language": "^0.19.7",
    "@codemirror/state": "^0.19.9",
    "@lezer/common": "^0.15.11",
    "@lezer/lr": "^0.15.8"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "@typescript-eslint/typescript-estree": "^5.12.1",
    "eslint": "^8.9.0",
    "eslint-plugin-compat": "^4.0.0",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-regexp": "^1.5.1",
    "eslint-plugin-tsdoc": "^0.2.14",
    "prettier": "^2.5.1",
    "prettier-plugin-jsdoc": "^0.3.30",
    "prettier-plugin-organize-imports": "^2.3.4",
    "typescript": "^4.5.5"
  }
}
